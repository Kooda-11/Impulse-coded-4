<!-- /sections/trustpilot-slider.liquid -->

   <div id="trustpilot-slider" class="trustpilot-banner">
    <div style="max-width: 1125px; margin: 0 auto;" class="trustpilot-banner-wrapper">
      <div class="trustpilot-slider-wrapper">
        <div class="trustpilot-box-before-slider">
          <div class="trustpilot-box-title">Uitstekend</div>
          <img src="https://cdn.shopify.com/s/files/1/0549/0117/8598/files/stars-4.5.svg?v=1662112912" loading="lazy" alt="" class="trustpilot-box-stars">
          <div class="trustpilot-box-review">Gebaseerd op <strong class="trustpilot-box-review-bold">742 reviews</strong></div>
          <img src="https://cdn.shopify.com/s/files/1/0667/6869/9616/files/Trustpilot-logo.svg?v=1667224289" loading="lazy" alt="" class="trustpilot-box-logo">
        </div>
        <div data-delay="4000" data-animation="slide" class="trustpilot-slider-desktop w-slider" data-autoplay="false" data-easing="ease" data-hide-arrows="false" data-disable-swipe="false" data-autoplay-limit="0" data-nav-spacing="3" data-duration="500" data-infinite="true">
          <div class="trustpilot-slider-mask w-slider-mask">
            <div class="trustbox-slider-slide w-slide">
              <div class="trustpilot-slider-slide">
                <div class="trustpilot-slider-card">
                  <div class="trustpilot-slider-card-top"><img loading="lazy" width="1250" src="https://cdn.shopify.com/s/files/1/0667/6869/9616/files/stars.svg?v=1667224289" alt="" class="trustpilot-stars">
                  {% include 'verified-badge' %}
                  </div>
                  <div class="trustpilot-slider-card-center">
                    <div class="trustpilot-slider-card-title">{{ section.settings.title-1 }}</div>
                    <p class="trustpilot-slider-card-text">{{ section.settings.review-1 }}</p>
                  </div>
                  <div class="trustpilot-slider-card-author">{{ section.settings.name-1 }}</div>
                </div>
                <div class="trustpilot-slider-card hide-mobile">
                  <div class="trustpilot-slider-card-top"><img loading="lazy" width="1250" src="https://cdn.shopify.com/s/files/1/0667/6869/9616/files/stars-4.svg?v=1667237928" alt="" class="trustpilot-stars">
                  {% include 'verified-badge' %}
                  </div>
                  <div class="trustpilot-slider-card-center">
